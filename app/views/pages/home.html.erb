<h1>Cronus</h1>
<div class="center">Archive of non-linear stories</div>
<div class='container'>
  <% @projects.each do |p| %>
    <div class="projects">
      <h3><%= link_to p.name, project_path(p) %></h3>
      <p><%= p.description %></p>
      <p><%= p.user.username %></p>
    </div>
  <% end %>
</div>
