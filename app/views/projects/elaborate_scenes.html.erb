<div class="container">
<h2><%= @project.name %></h2>

<% @project.scenes.each do |s| %>
  <% if s.editor_id == -1 %>
    <p>Start Scene | <%= link_to "Elaborate", edit_project_scene_path(@project, s) %></p>
  <% end %>
<% end %>

<% @project.scenes.each do |s| %>
  <ul>
  <% if s.editor_id != -1 %>
    <li class="sceneList"><%= s.summary %> | <%= link_to "Elaborate", edit_project_scene_path(@project, s) %></li>
  <% end %>
  </ul>
<% end %>
</div>
